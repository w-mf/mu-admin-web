{
  "name": "vitetemplate",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "npx vue-tsc --noEmit && vite build",
    "lint:eslint": "eslint --cache --max-warnings 0  src/**/*.{vue,ts}",
    "lint:eslint:fix": "eslint --cache --max-warnings 0  src/**/*.{vue,ts} --fix",
    "lint:stylelint": "stylelint --cache src/**/*.{vue,css,less} --cache --cache-location node_modules/.cache/stylelint/",
    "lint:stylelint:fix": "stylelint --cache --fix src/**/*.{vue,css,less} --cache --cache-location node_modules/.cache/stylelint/",
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "lint:fix": "npm run lint:eslint:fix && npm run lint:stylelint:fix",
    "preview": "vite preview",
    "api": "ytt && eslint src/api/*.ts --fix",
    "init": "npm install && husky install"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^1.1.3",
    "axios": "^0.26.1",
    "dayjs": "^1.11.0",
    "element-plus": "^2.1.4",
    "lodash": "^4.17.21",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@rushstack/eslint-patch": "^1.1.1",
    "@types/lodash": "^4.14.180",
    "@types/node": "^16.11.25",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "@vitejs/plugin-vue": "^2.2.4",
    "@vue/eslint-config-airbnb": "^6.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "commitlint-config-ali": "^0.1.3",
    "eslint": "^8.11.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "^7.0.4",
    "postcss-html": "^1.2.0",
    "postcss-scss": "^4.0.3",
    "prettier": "^2.6.0",
    "sass": "^1.49.9",
    "stylelint": "^14.6.0",
    "stylelint-config-ali": "^0.4.2",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.3.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.2.0",
    "typescript": "^4.6.2",
    "unplugin-auto-import": "^0.6.6",
    "unplugin-vue-components": "^0.18.3",
    "vite": "^2.8.6",
    "vue-eslint-parser": "^8.3.0",
    "vue-tsc": "^0.33.5",
    "yapi-to-typescript": "^3.33.2"
  }
}
