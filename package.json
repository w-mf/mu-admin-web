{
  "name": "vitetemplate",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "npx vue-tsc --noEmit && vite build",
    "lint:eslint": "eslint --cache --max-warnings 0  src/**/*.{vue,ts}",
    "lint:eslint:fix": "eslint --cache --max-warnings 0  src/**/*.{vue,ts} --fix",
    "lint:stylelint": "stylelint --cache src/**/*.{vue,css,less} --cache --cache-location node_modules/.cache/stylelint/",
    "lint:stylelint:fix": "stylelint --cache --fix src/**/*.{vue,css,less} --cache --cache-location node_modules/.cache/stylelint/",
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "lint:fix": "npm run lint:eslint:fix && npm run lint:stylelint:fix",
    "preview": "vite preview",
    "api": "ytt && eslint src/api/*.ts --fix",
    "init": "npm install && husky install"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^0.2.4",
    "axios": "^0.24.0",
    "element-plus": "^1.3.0-beta.5",
    "vue": "^3.2.26",
    "vue-router": "^4.0.12",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.2",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vitejs/plugin-vue": "^2.0.1",
    "commitlint-config-ali": "^0.1.3",
    "eslint": "^8.1.0",
    "eslint-config-ali": "^13.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.3.0",
    "husky": "^7.0.4",
    "postcss-html": "^1.2.0",
    "postcss-scss": "^4.0.3",
    "prettier": "^2.4.1",
    "sass": "^1.48.0",
    "stylelint": "^14.2.0",
    "stylelint-config-ali": "^0.4.1",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.1.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.1.0",
    "typescript": "^4.4.3",
    "unplugin-vue-components": "^0.17.0",
    "vite": "^2.7.12",
    "vue-eslint-parser": "^8.0.1",
    "vue-tsc": "^0.3.0",
    "yapi-to-typescript": "^3.26.2"
  }
}
